<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Mageplaza_Security::security">
                        <resource id="Mageplaza_SecurityPro::action_log" title="Action Log"/>
                        <resource id="Mageplaza_SecurityPro::action_log_backup" title="Action Log Backup"/>
                        <resource id="Mageplaza_SecurityPro::file_change" title="File Change Log"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
