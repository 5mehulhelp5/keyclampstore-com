<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="index">
        <job name="mageplaza_security_action_log_backup" instance="Mageplaza\SecurityPro\Cron\Backup" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="mageplaza_security_file_change" instance="Mageplaza\SecurityPro\Cron\FileChange" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
</config>
